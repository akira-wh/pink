{
  "name": "pink",
  "version": "0.1.0",
  "private": true,
  "description": "Проект «Пинк»",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.0.0",
    "autoprefixer": "9.5.1",
    "browser-sync": "2.26.5",
    "del": "4.1.1",
    "gulp": "4.0.2",
    "gulp-csso": "3.0.1",
    "gulp-imagemin": "6.0.0",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "8.0.0",
    "gulp-rename": "1.4.0",
    "gulp-sass": "4.0.2",
    "gulp-svgmin": "2.2.0",
    "gulp-svgstore": "7.0.1",
    "gulp-uglify": "3.0.2",
    "gulp-webp": "4.0.1",
    "stylelint": "10.0.1",
    "stylelint-config-htmlacademy": "0.1.3"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint sass/**/*.scss --syntax scss",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp build",
    "start": "npm run build && gulp browsersync"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.html",
    "*.json",
    "*.js",
    "js/**/*.js",
    "img/**/*.svg",
    "sass/**/*.{sass,scss}"
  ],
  "engines": {
    "node": "10.15.3"
  }
}
